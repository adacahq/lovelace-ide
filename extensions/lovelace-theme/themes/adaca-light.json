{
  "$schema": "vscode://schemas/color-theme",
  "name": "Adaca Light",
  "type": "light",
  "colors": {
    "editor.background": "#FFFFFF",
    "editor.foreground": "#2C3E50",
    "editor.lineHighlightBackground": "#2074EF08",
    "editor.selectionBackground": "#2074EF30",
    "editor.selectionHighlightBackground": "#2074EF15",
    "editor.findMatchBackground": "#2074EF50",
    "editor.findMatchHighlightBackground": "#2074EF20",
    "editor.hoverHighlightBackground": "#2074EF10",
    "editor.wordHighlightBackground": "#2074EF15",
    "editor.wordHighlightStrongBackground": "#2074EF30",
    "editorCursor.foreground": "#2074EF",
    "editorLineNumber.foreground": "#A0A0A0",
    "editorLineNumber.activeForeground": "#2074EF",
    "editorIndentGuide.background": "#E5E5E5",
    "editorIndentGuide.activeBackground": "#2074EF",
    "editorGroupHeader.tabsBackground": "#F8F9FA",
    "editorGroup.border": "#2074EF",
    "tab.activeBackground": "#FFFFFF",
    "tab.activeForeground": "#2C3E50",
    "tab.activeBorder": "#2074EF",
    "tab.inactiveBackground": "#F8F9FA",
    "tab.inactiveForeground": "#6C757D",
    "tab.border": "#E9ECEF",
    "sideBar.background": "#F8F9FA",
    "sideBar.foreground": "#495057",
    "sideBar.border": "#E9ECEF",
    "sideBarTitle.foreground": "#2C3E50",
    "sideBarSectionHeader.background": "#E9ECEF",
    "sideBarSectionHeader.foreground": "#2C3E50",
    "activityBar.background": "#E9ECEF",
    "activityBar.foreground": "#495057",
    "activityBar.activeBorder": "#2074EF",
    "activityBar.inactiveForeground": "#ADB5BD",
    "activityBarBadge.background": "#2074EF",
    "activityBarBadge.foreground": "#FFFFFF",
    "statusBar.background": "#E9ECEF",
    "statusBar.foreground": "#495057",
    "statusBar.debuggingBackground": "#2074EF",
    "statusBar.debuggingForeground": "#FFFFFF",
    "statusBarItem.hoverBackground": "#2074EF20",
    "titleBar.activeBackground": "#E9ECEF",
    "titleBar.activeForeground": "#2C3E50",
    "titleBar.inactiveBackground": "#DEE2E6",
    "titleBar.inactiveForeground": "#ADB5BD",
    "panel.background": "#F8F9FA",
    "panel.border": "#2074EF",
    "panelTitle.activeBorder": "#2074EF",
    "panelTitle.activeForeground": "#2C3E50",
    "panelTitle.inactiveForeground": "#6C757D",
    "terminal.background": "#FFFFFF",
    "terminal.foreground": "#2C3E50",
    "terminal.ansiBlue": "#2074EF",
    "terminal.ansiBrightBlue": "#0056D2",
    "list.activeSelectionBackground": "#2074EF40",
    "list.activeSelectionForeground": "#2C3E50",
    "list.inactiveSelectionBackground": "#2074EF20",
    "list.hoverBackground": "#2074EF15",
    "list.focusBackground": "#2074EF30",
    "list.highlightForeground": "#2074EF",
    "tree.indentGuidesStroke": "#E5E5E5",
    "button.background": "#2074EF",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#1A5FD9",
    "input.background": "#FFFFFF",
    "input.foreground": "#2C3E50",
    "input.border": "#2074EF",
    "input.placeholderForeground": "#ADB5BD",
    "inputOption.activeBorder": "#2074EF",
    "dropdown.background": "#FFFFFF",
    "dropdown.foreground": "#2C3E50",
    "dropdown.border": "#2074EF",
    "badge.background": "#2074EF",
    "badge.foreground": "#FFFFFF",
    "progressBar.background": "#2074EF",
    "breadcrumb.foreground": "#6C757D",
    "breadcrumb.background": "#FFFFFF",
    "breadcrumb.focusForeground": "#2C3E50",
    "breadcrumb.activeSelectionForeground": "#2074EF",
    "menu.background": "#FFFFFF",
    "menu.foreground": "#2C3E50",
    "menu.selectionBackground": "#2074EF",
    "menu.selectionForeground": "#FFFFFF",
    "menubar.selectionBackground": "#2074EF20",
    "menubar.selectionForeground": "#2C3E50",
    "notificationCenter.border": "#2074EF",
    "notifications.background": "#FFFFFF",
    "notifications.foreground": "#2C3E50",
    "notifications.border": "#2074EF",
    "notificationToast.border": "#2074EF",
    "extensionButton.prominentBackground": "#2074EF",
    "extensionButton.prominentForeground": "#FFFFFF",
    "extensionButton.prominentHoverBackground": "#1A5FD9",
    "pickerGroup.border": "#2074EF",
    "pickerGroup.foreground": "#2074EF",
    "debugToolBar.background": "#FFFFFF",
    "debugToolBar.border": "#2074EF",
    "welcomePage.buttonBackground": "#2074EF",
    "welcomePage.buttonHoverBackground": "#1A5FD9",
    "walkThrough.embeddedEditorBackground": "#FFFFFF",
    "gitDecoration.modifiedResourceForeground": "#E09D36",
    "gitDecoration.deletedResourceForeground": "#DC3545",
    "gitDecoration.untrackedResourceForeground": "#198754",
    "gitDecoration.ignoredResourceForeground": "#ADB5BD",
    "gitDecoration.conflictingResourceForeground": "#DC3545"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6A9955"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#2C3E50"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#DC3545"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#2074EF"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#0969DA"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#B31D28"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#953800"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": [
        "meta.block variable.other"
      ],
      "settings": {
        "foreground": "#B31D28"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#B31D28"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#0550AE"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#0A3069"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#953800"
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "support.type"
      ],
      "settings": {
        "foreground": "#0550AE"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#0550AE"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#DC3545"
      }
    },
    {
      "name": "Language methods",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#DC3545"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": [
        "entity.name.method.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#953800"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#953800"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#8250DF"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#953800"
      }
    },
    {
      "name": "CSS Classes",
      "scope": [
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "#953800"
      }
    },
    {
      "name": "CSS ID's",
      "scope": [
        "source.sass keyword.control"
      ],
      "settings": {
        "foreground": "#953800"
      }
    },
    {
      "name": "Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#0A3069"
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#DC3545"
      }
    },
    {
      "name": "Changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#8250DF"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#0969DA"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#0969DA"
      }
    },
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#953800"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#DC3545"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#8250DF"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#953800"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#0550AE"
      }
    },
    {
      "name": "JSON Key - Level 3",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#DC3545"
      }
    },
    {
      "name": "Markdown - Plain",
      "scope": [
        "text.html.markdown",
        "punctuation.definition.list_item.markdown"
      ],
      "settings": {
        "foreground": "#24292F"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": [
        "text.html.markdown markup.inline.raw.markdown"
      ],
      "settings": {
        "foreground": "#8250DF"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline Punctuation",
      "scope": [
        "text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"
      ],
      "settings": {
        "foreground": "#656D76"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#0A3069"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#B31D28"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": [
        "markup.bold",
        "markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#B31D28"
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#B31D28"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#0550AE"
      }
    },
    {
      "name": "Markdown - Blockquote",
      "scope": [
        "markup.quote punctuation.definition.blockquote.markdown"
      ],
      "settings": {
        "foreground": "#656D76"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": [
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#953800"
      }
    },
    {
      "name": "Markdown - Link Description",
      "scope": [
        "string.other.link.description.title.markdown"
      ],
      "settings": {
        "foreground": "#8250DF"
      }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": [
        "constant.other.reference.link.markdown"
      ],
      "settings": {
        "foreground": "#953800"
      }
    },
    {
      "name": "Markup - Raw Block",
      "scope": [
        "markup.raw.block"
      ],
      "settings": {
        "foreground": "#8250DF"
      }
    },
    {
      "name": "Markdown - Raw Block Fenced",
      "scope": [
        "markup.raw.block.fenced.markdown"
      ],
      "settings": {
        "foreground": "#656D7650"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block",
      "scope": [
        "punctuation.definition.fenced.markdown"
      ],
      "settings": {
        "foreground": "#656D7650"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block Variable",
      "scope": [
        "markup.raw.block.fenced.markdown",
        "variable.language.fenced.markdown",
        "punctuation.section.class.end"
      ],
      "settings": {
        "foreground": "#24292F"
      }
    },
    {
      "name": "Markdown - Fenced Language",
      "scope": [
        "variable.language.fenced.markdown"
      ],
      "settings": {
        "foreground": "#656D76"
      }
    },
    {
      "name": "Markdown - Separator",
      "scope": [
        "meta.separator"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#656D76"
      }
    },
    {
      "name": "Markup - Table",
      "scope": [
        "markup.table"
      ],
      "settings": {
        "foreground": "#24292F"
      }
    }
  ]
}